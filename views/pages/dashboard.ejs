<%- include('../partials/head') %>
<%- include('../partials/sidenav') %>
  <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">
    <!-- Navbar -->
    <%- include('../partials/navbar') %>
    <!-- End Navbar -->
    <div class="container-fluid py-4">
      <div class="row">
        <div class="col-lg-4 col-md-6 mt-4 mb-4">
          <div class="card">
            <div class="card-header p-3 pt-2">
              <div class="icon icon-lg icon-shape bg-gradient-success shadow-dark text-center border-radius-xl mt-n4 position-absolute">
                <i class="material-icons opacity-10">people</i>
              </div>
              <div class="text-end pt-1">
                <p class="text-sm mb-0 text-capitalize">Total Conversions</p>
                <h4 id="lblConversions" class="mb-0"></h4>
              </div>
            </div>
            <hr class="dark horizontal my-0">
            <div class="card-footer p-3">
              <p class="mb-0"><span class="text-success text-sm font-weight-bolder"></span>Total Conversions based on selection date range</p>
            </div>
          </div>
        </div>
        <div class="col-lg-4 col-md-6 mt-4 mb-4">
          <div class="card">
            <div class="card-header p-3 pt-2">
              <div class="icon icon-lg icon-shape bg-gradient-primary shadow-primary text-center border-radius-xl mt-n4 position-absolute">
                <i class="material-icons opacity-10">person</i>
              </div>
              <div class="text-end pt-1">
                <p class="text-sm mb-0 text-capitalize">Full Journey</p>
                <h4 class="mb-0" id="lblFullJourneyConversion"></h4>
              </div>
            </div>
            <hr class="dark horizontal my-0">
            <div class="card-footer p-3">
              <p class="mb-0"><span class="text-success text-sm font-weight-bolder"></span>No of customer started and converted based on selected date range</p>
            </div>
          </div>
        </div>
        <div class="col-lg-4 col-md-6 mt-4 mb-4">
          <div class="card">
            <div class="card-header p-3 pt-2">
              <div class="icon icon-lg icon-shape bg-gradient-success shadow-success text-center border-radius-xl mt-n4 position-absolute">
                <i class="material-icons opacity-10">person</i>
              </div>
              <div class="text-end pt-1">
                <p class="text-sm mb-0 text-capitalize">Carried Forward</p>
                <h4 class="mb-0" id="lblCarryForwardConversion"></h4>
              </div>
            </div>
            <hr class="dark horizontal my-0">
            <div class="card-footer p-3">
              <p class="mb-0"><span class="text-danger text-sm font-weight-bolder"></span>Customer started before your selection date</p>
            </div>
          </div>
        </div>
       
      </div>
      <div class="row mt-4"></div>
        <div class="col-lg-12 mb-6">
          <div class="card z-index-2  ">
            <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2 bg-transparent">
              <div class="bg-gradient-dark shadow-success border-radius-lg py-3 pe-1">
                <div class="chart">
                  <canvas id="chart-dropoff" class="chart-dropoff" height="170"></canvas>
                </div>
              </div>
            </div>
            <div class="card-body">
              <h6 class="mb-0 "> SavePlus Drop-Off Events Analysis</h6>
              <p class="text-sm "> <span class="font-weight-bolder"></span>Events 1-17 </p>
              <hr class="dark horizontal">
              <div class="d-flex ">
                <i class="material-icons text-sm my-auto me-1"></i>
                <p class="mb-0 text-sm">  </p>
              </div>
            </div>
          </div>
      </div>
      <div class="row mt-4"></div>
        <div class="col-lg-12 mb-6">
          <div class="card z-index-2  ">
            <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2 bg-transparent">
              <div class="bg-gradient-dark shadow-success border-radius-lg py-3 pe-1">
                <div class="chart">
                  <canvas id="chart-dropoff2" class="chart-dropoff" height="170"></canvas>
                </div>
              </div>
            </div>
            <div class="card-body">
              <h6 class="mb-0 "> SavePlus Drop-Off Events Analysis</h6>
              <p class="text-sm "> <span class="font-weight-bolder"></span>Events 18-36</p>
              <hr class="dark horizontal">
              <div class="d-flex ">
                <i class="material-icons text-sm my-auto me-1"></i>
                <p class="mb-0 text-sm">  </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
      
      <%- include('../partials/footer') %>
    </div>
  </main>
  <%- include('../partials/dashjs') %>
  <%- include('../partials/end') %>